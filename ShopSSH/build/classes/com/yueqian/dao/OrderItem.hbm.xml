<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.shop.domain.OrderItem" table="orderitem">
		<id name="itemid" column="itemid"></id>
		<property name="count" column="count"></property>
		<property name="subtotal" column="subtotal"></property>
		<property name="pid" type="java.lang.String" column="pid"></property>
		<property name="oid" type="java.lang.String" column="oid"></property>
	</class>
</hibernate-mapping>